                <defs>
                    <pattern id="64e643ad-2176-4f86-b3d7-f2c5da3b6a6d" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
                        <rect x="0" y="0" width="4" height="4" class="text-warm-gray-200" fill="currentColor"></rect>
                    </pattern>
                </defs>
                <rect width="404" height="384" fill="url(#64e643ad-2176-4f86-b3d7-f2c5da3b6a6d)"></rect>
            </svg>
        </div>
        <div
            id="contact-form"
            class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8"
        >
            <div class="relative bg-white shadow-xl">
                <h2 id="contact-heading" class="sr-only">Contáctanos</h2>

                <div class="grid grid-cols-1 lg:grid-cols-3">
                    <!-- Contact information -->
                    <div class="relative overflow-hidden py-10 px-6 bg-gradient-to-b from-{{ settings.highlightColor }}-500 to-{{ settings.highlightColor }}-600 sm:px-10 xl:p-12">
