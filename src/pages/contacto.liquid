                        </ul>
                    </div>

                    <!-- Contact form -->
                    <div
                        class="py-10 px-6 sm:px-10 lg:col-span-2 xl:p-12"
                        x-data="{}"
                    >
                        <h3 class="text-lg font-medium text-warm-gray-900">Envíanos un mensaje</h3>
                        <form
                            action="/exito"
                            method="POST"
                            class="mt-6 grid grid-cols-1 gap-y-6 sm:grid-cols-2 sm:gap-x-8"
                            data-netlify="true"
                            novalidate
                            data-validation='needs-validation'
                            name="contacto"
                        >
                            <style type="text/css" scoped>
                                .was-validated .validation-feedback {
                                    visibility: visible;
                                }

                                .validation-feedback {
                                    visibility: hidden;
                                }
                            </style>
                            <div>
                                <label for="first-name" class="block text-sm font-medium text-warm-gray-900">Nombre</label>
                                <div class="mt-1 relative">
                                    <input
                                        type="text"
                                        name="first-name"
                                        id="first-name"
                                        autocomplete="given-name"
                                        class="py-3 px-4 block w-full shadow-md text-warm-gray-900 focus:ring-{{ settings.highlightColor }}-500 focus:border-{{ settings.highlightColor }}-500 border-warm-gray-300 rounded-md"
                                        required
                                    >
                                    <div class="validation-feedback absolute inset-y-0 right-0 pr-3 flex items-center pointer-events-none">
                                        {{heroIcons.solidExclamationCircle}}
                                    </div>
                                </div>
                                <p class="validation-feedback mt-2 text-sm text-red-600" id="email-error">Escribe tu nombre.</p>
                            </div>
                            <div>
                                <label for="last-name" class="block text-sm font-medium text-warm-gray-900">Apellido</label>
                                <div class="mt-1">
                                    <input
                                        type="text"
                                        name="last-name"
                                        id="last-name"
                                        autocomplete="family-name"
                                        class="py-3 px-4 block w-full shadow-md text-warm-gray-900 focus:ring-{{ settings.highlightColor }}-500 focus:border-{{ settings.highlightColor }}-500 border-warm-gray-300 rounded-md"
                                    >
                                </div>
                            </div>
                            <div class="sm:col-span-2">
                                <label for="email" class="block text-sm font-medium text-warm-gray-900">Correo Electrónico</label>
                                <div class="mt-1 relative">
                                    <input
                                        id="email"
                                        name="email"
                                        type="email"
                                        autocomplete="email"
                                        class="py-3 px-4 block w-full shadow-md text-warm-gray-900 focus:ring-{{ settings.highlightColor }}-500 focus:border-{{ settings.highlightColor }}-500 border-warm-gray-300 rounded-md"
                                        required
                                    >
                                    <div class="validation-feedback absolute inset-y-0 right-0 pr-3 flex items-center pointer-events-none">
                                        {{heroIcons.solidExclamationCircle}}
                                    </div>
                                </div>
                                <p class="validation-feedback mt-2 text-sm text-red-600" id="email-error">Escribe tu dirección de correo electrónico.</p>
                            </div>
                            <div class="sm:col-span-2">
                                <label for="phone" class="block text-sm font-medium text-warm-gray-900">Teléfono</label>
                                <div class="mt-1 relative">
                                    <input
                                        id="phone"
                                        name="phone"
                                        type="tel"
                                        class="py-3 px-4 block w-full shadow-md text-warm-gray-900 focus:ring-{{ settings.highlightColor }}-500 focus:border-{{ settings.highlightColor }}-500 border-warm-gray-300 rounded-md"
                                    >
                                </div>
                            </div>
                            <div class="sm:col-span-2">
                                <div class="flex justify-between">
                                    <label for="message" class="block text-sm font-medium text-warm-gray-900">Mensaje</label>
                                    <span id="message-max" class="text-sm text-warm-gray-500">Max. 500 caracteres</span>
                                </div>
                                <div class="mt-1 relative">
                                    <textarea
                                        id="message"
                                        name="message"
                                        rows="4"
                                        class="py-3 px-4 block w-full shadow-md text-warm-gray-900 focus:ring-{{ settings.highlightColor }}-500 focus:border-{{ settings.highlightColor }}-500 border border-warm-gray-300 rounded-md"
                                        aria-describedby="message-max"
                                        required
                                    >
                                    </textarea>
                                    <div class="validation-feedback absolute inset-y-0 right-0 pr-3 flex items-center pointer-events-none">
                                        {{heroIcons.solidExclamationCircle}}
                                    </div>
                                </div>
                                <p class="validation-feedback mt-2 text-sm text-red-600" id="email-error">Escribe tu mensaje.</p>
                            </div>
                            <div class="sm:col-span-2 sm:flex sm:justify-end">
                                <button type="submit" class="mt-2 w-full inline-flex items-center justify-center px-6 py-3 border border-transparent rounded-md shadow-sm text-base font-medium text-white bg-{{ settings.highlightColor }}-500 hover:bg-{{ settings.highlightColor }}-600 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-{{ settings.highlightColor }}-500 sm:w-auto">
                                    Enviar
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Contact grid -->
    <section aria-labelledby="offices-heading">
        <div class="max-w-7xl mx-auto py-24 px-4 sm:py-32 sm:px-6 lg:px-8">
            <h2 id="offices-heading" class="text-3xl font-extrabold text-warm-gray-900">Soporte de Ventas</h2>
            <p class="mt-6 text-lg text-warm-gray-500 max-w-3xl">Llama a nuestro Departamento de Atención a Clientes:</p>
            <div class="mt-10 grid grid-cols-1 gap-10 sm:grid-cols-2 lg:grid-cols-4">

                <div>
                    <h3 class="text-lg font-medium text-warm-gray-900">CDMX y Área Metropolitana</h3>
                    <p class="mt-2 text-base text-warm-gray-500">

                        <span class="block">55-36-00-96-00</span>

                    </p>
                </div>

                <div>
                    <h3 class="text-lg font-medium text-warm-gray-900">Interior de la República</h3>
                    <p class="mt-2 text-base text-warm-gray-500">

                        <span class="block">800-10-10-053</span>

                    </p>
                </div>

            </div>
        </div>
    </section>
    <script type="text/javascript" src="/static/js/jquery-3.6.0.slim.min.js"></script>
    <script type="text/javascript" src="/static/js/main.js"></script>
</main>

